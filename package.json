    "react-redux": "^7.2.1",
    "react-scripts": "3.4.3",
    "react-viewport-hooks": "1.5.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "typescript": "4.0.3",
    "use-debounce": "^5.0.1"
