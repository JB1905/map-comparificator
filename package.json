    "i18next-http-backend": "^1.0.21",
    "leaflet": "^1.7.1",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-bingmaps-plus": "^3.7.2",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.7.3",
    "react-leaflet": "^2.7.0",
    "react-map-gl": "^5.2.8",
    "react-mapkit": "^0.6.5",
