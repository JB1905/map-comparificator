{
  "name": "map-comparificator",
  "version": "1.0.0",
  "descripton": "Choose the best map for your project",
  "license": "MIT",
  "private": true,
  "homepage": ".",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eslint": "eslint 'src/**/*.{tsx,ts}'",
    "prettier": "prettier --write '{src,public}/**/*.{tsx,ts,scss,html}'",
    "test": "react-scripts test --coverage --env=jest-environment-jsdom-sixteen",
    "cy:open": "cypress open",
    "eject": "react-scripts eject",
    "commit": "git-cz",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.48.0",
    "@blueprintjs/icons": "^3.28.0",
    "@blueprintjs/popover2": "^0.11.2",
    "@blueprintjs/select": "^3.17.0",
    "@react-google-maps/api": "2.1.1",
    "deep-equal": "^2.0.5",
    "i18next": "^20.4.0",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-http-backend": "^1.3.0",
    "is-mobile": "^3.0.0",
    "leaflet": "^1.7.1",
    "node-sass": "^5.0.0",
    "react": "^17.0.2",
    "react-bingmaps-plus": "^3.7.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hotkeys-hook": "^3.4.0",
    "react-i18next": "^11.11.4",
    "react-leaflet": "^3.1.0",
    "react-map-gl": "^6.1.12",
    "react-mapkit": "^0.6.5",
    "react-mosaic-component": "^4.1.1",
    "react-redux": "^7.2.4",
    "react-scripts": "4.0.3",
    "react-viewport-hooks": "1.5.0",
    "redux": "^4.1.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "typescript": "^4.2.4",
    "use-debounce": "^6.0.1",
    "web-vitals": "^1.1.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/react-hooks": "^5.1.1",
    "@testing-library/user-event": "^13.1.3",
    "@types/deep-equal": "^1.0.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^16.7.1",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.2",
    "@types/react-leaflet": "^2.8.2",
    "@types/react-map-gl": "^5.2.10",
    "@types/react-redux": "^7.1.18",
    "cz-conventional-changelog": "3.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "husky": "^7.0.1",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:cypress/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "{src,public}/**/*.{tsx,ts,scss,html}": [
      "npm run prettier"
    ],
    "src/**/*.{tsx,ts}": [
      "npm run eslint"
    ]
  }
}
