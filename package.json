{
  "name": "map-comparificator",
  "version": "1.0.0",
  "descripton": "Choose the best map for your project",
  "license": "MIT",
  "private": true,
  "homepage": ".",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eslint": "eslint 'src/**/*.{tsx,ts}'",
    "prettier": "prettier --write '{src,public}/**/*.{tsx,ts,scss,html}'",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "commit": "git-cz"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.36.0",
    "@blueprintjs/icons": "^3.23.0",
    "@blueprintjs/select": "^3.15.0",
    "@impargo/react-here-maps": "^1.6.0",
    "@react-google-maps/api": "2.0.2",
    "deep-equal": "^2.0.5",
    "i18next": "^19.8.4",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-http-backend": "^1.0.21",
    "leaflet": "^1.7.1",
    "node-sass": "^4.14.1",
    "react": "^17.0.1",
    "react-bingmaps-plus": "^3.7.2",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-hotkeys-hook": "^3.0.0",
    "react-i18next": "^11.8.4",
    "react-leaflet": "^2.8.0",
    "react-map-gl": "^6.0.2",
    "react-mapkit": "^0.6.5",
    "react-mosaic-component": "^4.1.1",
    "react-redux": "^7.2.2",
    "react-scripts": "4.0.1",
    "react-viewport-hooks": "1.5.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "typescript": "^4.1.3",
    "use-debounce": "^5.2.0",
    "use-throttle": "^0.0.3",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "^3.7.0",
    "@testing-library/user-event": "^12.6.0",
    "@types/deep-equal": "^1.0.1",
    "@types/googlemaps": "^3.43.0",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.16",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/react-leaflet": "^2.5.2",
    "@types/react-map-gl": "^5.2.9",
    "@types/react-redux": "^7.1.14",
    "cz-conventional-changelog": "3.3.0",
    "husky": "^4.3.6",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,public}/**/*.{tsx,ts,scss,html}": [
      "npm run prettier"
    ],
    "src/**/*.{tsx,ts}": [
      "npm run eslint"
    ]
  }
}
