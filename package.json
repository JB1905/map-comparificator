  "private": true,
  "homepage": ".",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "prettier": "prettier --write '{src,public}/**/*.{tsx,ts,scss,html}'",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
